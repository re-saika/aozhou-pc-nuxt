  "config": {
    "nuxt": {
      "port": 39000,
      "host": "0.0.0.0"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "iview": "^3.1.5",
    "nuxt": "^2.0.0",
    "swiper": "^5.4.2",
    "v-clipboard": "^2.2.3",
    "v-viewer": "^1.5.1",
    "vue": "latest",
    "vue-awesome-swiper": "^4.1.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
